{
  "expo": {
    "name": "3.Garden",
    "slug": "garden-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/a.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/a.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ckavi2001.x3.Garden"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d37bee75-ab8c-4f78-8e9c-04cd5c829d5e"
      }
    },
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-responsive-screen",
          "react-native-indicators",
          "react-native-status-bar-height",
          "firebase"
        ]
      }
    }
  }
}
